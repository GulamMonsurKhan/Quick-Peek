{
  "name": "Qwiki",
  "description": "Search while keeping context",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://en.wikipedia.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}